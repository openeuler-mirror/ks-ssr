<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<br_rs>
    <head>
        <name>CUSTOM_CLASS_I_SAFETY_STANDARD</name>
        <label>QT_TRANSLATE_NOOP_UTF8("xml", "Level I safety standard")</label>
        <version>1.0.0</version>
    </head>
    <body>
        <!-- 网络类 -->
        <reinforcement>
            <name>network-sysctl-icmp-redirect</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Turn on ICMP redirection")</label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>network-sysctl-syn-flood</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Prevent syn flood attacks")</label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>network-sysctl-source-route</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Turn on IP source routing")</label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>network-icmp-timestamp</name>
            <arg>
                <name>timestamp_request</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Allow ICMP timestamp query")</label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>network-icmp-traceroute</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Prohibit the host from being detected by traceroute")</label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>network-firewalld-switch</name>
            <!-- <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Turn on system firewall service")</label>
                </layout>
            </arg> -->
            <arg>
                <name>tcp-udp</name>
                <value>"tcp"</value>
                <value_limits>(tcp)|(udp)</value_limits>
                <input_example>tcp || udp</input_example>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>"tcp"</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "transport protocol")</label>
                </layout>
            </arg>
            <arg>
                <name>threat-port</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Disable ports that may threaten the system by default")</label>
                </layout>
            </arg>
            <arg>
                <name>clear-configuration</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "clear iptables configuration")</label>
                </layout>
                <note>QT_TRANSLATE_NOOP_UTF8("xml", "If this configuration is enabled, all firewall configurations will be cleared, and the firewall rules reinforced this time will not take effect")</note>
            </arg>
            <!-- <arg>
                <name>allow-network-segment</name>
                <value>""</value>
                <value_limits>(((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\/((3[0-2])|[0-2]?\d{1}))){1}(,((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\/((3[0-2])|[0-2]?\d{1})))*</value_limits>
                <input_example>192.168.163.1/24</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "allow network segment (Split with comma)")</label>
                </layout>
            </arg> -->
            <arg>
                <name>disable-network-segment</name>
                <value>""</value>
                <value_limits>(((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\/((3[0-2])|[0-2]?\d{1}))){1}(,((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\/((3[0-2])|[0-2]?\d{1})))*</value_limits>
                <input_example>192.168.163.1/24</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "disable network segment (Split with comma)")</label>
                </layout>
            </arg>
            <!-- <arg>
                <name>ports</name>
                <value>""</value>
                <value_limits>(((60[0-9]{3})|([1-5][0-9]{4})|([1-9][0-9]{0,3}))){1}(;((60[0-9]{3})|([1-5][0-9]{4})|([1-9][0-9]{0,3})))*</value_limits>
                <input_example>1~60999</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Opened ports (Split with semicolon)")</label>
                </layout>
            </arg> -->
            <arg>
                <name>disable-ports</name>
                <value>""</value>
                <value_limits>(((60[0-9]{3})|([1-5][0-9]{4})|([1-9][0-9]{0,3}))){1}(;((60[0-9]{3})|([1-5][0-9]{4})|([1-9][0-9]{0,3})))*</value_limits>
                <input_example>1~60999</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "disable ports (Split with semicolon)")</label>
                </layout>
            </arg>
            <arg>
                <name>input-ports-connect-nums</name>
                <value>0</value>
                <value_limits>-1</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <max_value>0</max_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Limit input port maximum connections(0 means unlimited)")</label>
                </layout>
            </arg>
            <!-- <arg>
                <name>allow-network-segment-output</name>
                <value>""</value>
                <value_limits>(((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\/((3[0-2])|[0-2]?\d{1}))){1}(,((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\/((3[0-2])|[0-2]?\d{1})))*</value_limits>
                <input_example>192.168.163.1/24</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "allow output network segment (Split with comma)")</label>
                </layout>
            </arg> -->
            <arg>
                <name>disable-network-segment-output</name>
                <value>""</value>
                <value_limits>(((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\/((3[0-2])|[0-2]?\d{1}))){1}(,((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2}))(\/((3[0-2])|[0-2]?\d{1})))*</value_limits>
                <input_example>192.168.163.1/24</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "disable output network segment (Split with comma)")</label>
                </layout>
            </arg>
            <!-- <arg>
                <name>ports-output</name>
                <value>""</value>
                <value_limits>(((60[0-9]{3})|([1-5][0-9]{4})|([1-9][0-9]{0,3}))){1}(;((60[0-9]{3})|([1-5][0-9]{4})|([1-9][0-9]{0,3})))*</value_limits>
                <input_example>1~60999</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Opened output ports (Split with semicolon)")</label>
                </layout>
            </arg> -->
            <arg>
                <name>disable-ports-output</name>
                <value>""</value>
                <value_limits>(((60[0-9]{3})|([1-5][0-9]{4})|([1-9][0-9]{0,3}))){1}(;((60[0-9]{3})|([1-5][0-9]{4})|([1-9][0-9]{0,3})))*</value_limits>
                <input_example>1~60999</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "disable output ports (Split with semicolon)")</label>
                </layout>
            </arg>
            <arg>
                <name>disable-ping</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "disable icmp ping")</label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>network-sysctl-high-risk-vulnerability</name>
            <arg>
                <name>bluetooth</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Turn on system bluetooth service")</label>
                </layout>
            </arg>
            <arg>
                <name>cups</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Turn on system cups service")</label>
                </layout>
            </arg>
            <arg>
                <name>avahi-daemon</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Turn on system avahi-daemon service")</label>
                </layout>
            </arg>
            <arg>
                <name>rpcbind</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Turn on system rpcbind service")</label>
                </layout>
            </arg>
            <arg>
                <name>smb</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Turn on system smb service")</label>
                </layout>
            </arg>
        </reinforcement>
        <!-- 配置类 -->
        <reinforcement>
            <name>config-sendmail-switch</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Turn on sendmail service")</label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-history-size</name>
            <arg>
                <name>HISTSIZE</name>
                <value>5</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <max_value>5</max_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Number of historical commands")</label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-file-permissions</name>
            <arg>
                <name>mode-permissions-limit</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "The maximum permissions of important configuration files in the system is set to 0644")</label>
                </layout>
                <note>QT_TRANSLATE_NOOP_UTF8("xml", "The maximum permissions for configuration files /etc/passwd, /etc/bashrc/etc/shadow, /etc/group, /etc/security/limits.conf, /etc/pam.d/system-auth-ac, /etc/fstab are set to 0644")</note>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-directory-permissions</name>
            <arg>
                <name>directory-permissions-limit</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "The maximum permission of key directories in the system is 0755")</label>
                </layout>
                <note>QT_TRANSLATE_NOOP_UTF8("xml", "The permissions for the/usr,/usr/bin,/usr/sbin,/usr/lib,/usr/lib64,/etc,/var directories in the system are set to 0755")</note>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-key-reboot</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Turn on composite key reboot")</label>
                </layout>
                <note>QT_TRANSLATE_NOOP_UTF8("xml", "The restart function is applied to the non graphical interface, and in the graphical interface, it acts as shutdown, and the function takes effect after logging out of the user.")</note>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-login-lock</name>
            <arg>
                <name>deny</name>
                <value>3</value>
                <value_limits>1</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <max_value>3</max_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Password error times")</label>
                </layout>
            </arg>
            <arg>
                <name>unlock_time</name>
                <value>300</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>300</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Automatic unlocking time after locking (Sec)")</label>
                </layout>
            </arg>
            <arg>
                <name>even_deny_root</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Root login failed lock")</label>
                </layout>
            </arg>
            <arg>
                <name>root_unlock_time</name>
                <value>300</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>300</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Unlock time after root user locks (Sec)")</label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-password-expired</name>
            <arg>
                <name>PASS_MAX_DAYS</name>
                <value>90</value>
                <value_limits>-1</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <max_value>90</max_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Maximum number of valid days for new users to use passwords")</label>
                </layout>
            </arg>
            <arg>
                <name>PASS_MIN_DAYS</name>
                <value>1</value>
                <value_limits>-1</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>1</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Minimum number of days allowed between password changes")</label>
                </layout>
            </arg>
            <!-- <arg>
                <name>PASS_MIN_LEN</name>
                <value>8</value>
                <value_limits>6</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>8</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Minimum acceptable password length")</label>
                </layout>
            </arg> -->
            <arg>
                <name>PASS_WARN_AGE</name>
                <value>28</value>
                <value_limits>-1</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>28</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Number of days warning given before a password expires")</label>
                </layout>
            </arg>
            <arg>
                <name>accounts_expiration</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Whether to set the expiration time for the current existing account")</label>
                </layout>
            </arg>
            <arg>
                <name>accounts</name>
                <value>90</value>
                <value_limits>-1</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <max_value>90</max_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Existing users set expiration days")</label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-password-complexity</name>
            <arg>
                <name>minlen</name>
                <value>8</value>
                <value_limits>6</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>8</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <label>QT_TRANSLATE_NOOP_UTF8("xml", "Minimum password length")</label>
                </layout>
            </arg>
