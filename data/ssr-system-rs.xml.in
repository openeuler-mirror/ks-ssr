<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<ssr_rs>
    <head>
        <name>CUSTOM_CLASS_I_SAFETY_STANDARD</name>
        <_label>Level I safety standard</_label>
        <version>1.0.0</version>
    </head>
    <body>
        <!-- 网络类 -->
        <reinforcement>
            <name>network-firewalld-switch</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on system firewall service</_label>
                </layout>
            </arg>
            <arg>
                <name>ports</name>
                <value>""</value>
                <value_limits>((60[0-9]{3})|([1-5][0-9]{4})|([1-9][0-9]{0,3}))((\/tcp)|(\/udp))(;((60[0-9]{3})|([1-5][0-9]{4})|([1-9][0-9]{0,3}))((\/tcp)|(\/udp)))*</value_limits>
                <input_example>1~60999/(tcp|udp)</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Opened ports (Split with semicolon)</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>network-sysctl-icmp-redirect</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on ICMP redirection</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>network-sysctl-syn-flood</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Prevent syn flood attacks</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>network-sysctl-source-route</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on IP source routing</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>network-firewalld-icmp-timestamp</name>
            <arg>
                <name>timestamp_request</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow ICMP timestamp query</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>network-sysctl-high-risk-vulnerability</name>
            <arg>
                <name>bluetooth</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on system bluetooth service</_label>
                </layout>
            </arg>
            <arg>
                <name>cups</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on system cups service</_label>
                </layout>
            </arg>
            <arg>
                <name>avahi-daemon</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on system avahi-daemon service</_label>
                </layout>
            </arg>
            <arg>
                <name>rpcbind</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on system rpcbind service</_label>
                </layout>
            </arg>
            <arg>
                <name>smb</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on system smb service</_label>
                </layout>
            </arg>
        </reinforcement>
        <!-- 配置类 -->
        <reinforcement>
            <name>config-sendmail-switch</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on sendmail service</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-history-size</name>
            <arg>
                <name>HISTSIZE</name>
                <value>5</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <max_value>5</max_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Number of historical commands</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-file-permissions</name>
            <arg>
                <name>mode-permissions-limit</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>The maximum permissions of important configuration files in the system is set to 0644</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-key-reboot</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on composite key reboot</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-login-lock</name>
            <arg>
                <name>deny</name>
                <value>3</value>
                <value_limits>1</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <max_value>3</max_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Password error times</_label>
                </layout>
            </arg>
            <arg>
                <name>unlock_time</name>
                <value>300</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>300</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Automatic unlocking time after locking (Sec)</_label>
                </layout>
            </arg>
            <arg>
                <name>even_deny_root</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Root login failed lock</_label>
                </layout>
            </arg>
            <arg>
                <name>root_unlock_time</name>
                <value>300</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>300</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Unlock time after root user locks (Sec)</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-password-expired</name>
            <arg>
                <name>PASS_MAX_DAYS</name>
                <value>180</value>
                <value_limits>-1</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <max_value>180</max_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Maximum number of days a password may be used</_label>
                </layout>
            </arg>
            <arg>
                <name>PASS_MIN_DAYS</name>
                <value>1</value>
                <value_limits>-1</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>1</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Minimum number of days allowed between password changes</_label>
                </layout>
            </arg>
            <!-- <arg>
                <name>PASS_MIN_LEN</name>
                <value>8</value>
                <value_limits>6</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>8</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Minimum acceptable password length</_label>
                </layout>
            </arg> -->
            <arg>
                <name>PASS_WARN_AGE</name>
                <value>28</value>
                <value_limits>-1</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>28</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Number of days warning given before a password expires</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-password-complexity</name>
            <arg>
                <name>minlen</name>
                <value>8</value>
                <value_limits>6</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>8</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Minimum password length</_label>
                </layout>
            </arg>
            <arg>
                <name>ucredit</name>
                <value>0</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>0</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Number of capital letters</_label>
                </layout>
            </arg>
            <arg>
                <name>lcredit</name>
                <value>0</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>0</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Number of lowercase letters</_label>
                </layout>
            </arg>
            <arg>
                <name>dcredit</name>
                <value>1</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>1</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Number of digits</_label>
                </layout>
            </arg>
            <arg>
                <name>ocredit</name>
                <value>1</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>1</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Number of special characters</_label>
                </layout>
            </arg>
            <arg>
                <name>minclass</name>
                <value>3</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>3</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Contains number of character types (special characters, numbers, letters)</_label>
                </layout>
            </arg>
            <arg>
                <name>maxsequence</name>
                <value>3</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>3</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Maximum number of consecutive characters</_label>
                </layout>
            </arg>
            <arg>
                <name>usercheck</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow user names</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-resource-limits</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Stack and RSS sizes are limited (10240)</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-sak-key</name>
            <arg>
                <name>kernel.sysrq</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on SAK key</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-umask-limit</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>File directory default permission</_label>
                </layout>
            </arg>
        </reinforcement>
        <!-- 审计类 -->
        <reinforcement>
            <name>audit-auditd-switch</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on system auditd service</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>audit-logrotate-rotate</name>
            <arg>
                <name>rotate</name>
                <value>24</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>24</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>System log save cycle（weekly）</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>audit-logfile-permissions</name>
            <arg>
                <name>mode-permissions-limit</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>The maximum permission of log files and configurations is set to 0644</_label>
                </layout>
            </arg>
            <arg>
                <name>append-permissions-limit</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Log files can only be appended</_label>
                </layout>
            </arg>
        </reinforcement>
        <!-- 接入类 -->
        <reinforcement>
            <name>external-udev-cdrom</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow to use CD-ROM device</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-udev-usb</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow to use USB device</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-udev-ttys</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow to use serial port device</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-accounts-login-limit</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Restrict system independent user login</_label>
                </layout>
            </arg>
            <arg>
                <name>permission-users</name>
                <value>""</value>
                <value_limits>[a-zA-Z0-9_.][a-zA-Z0-9_.-]*[$]?([;][a-zA-Z0-9_.][a-zA-Z0-9_.-]*[$]?)*</value_limits>
                <input_example>name1;name2...</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow account retention (Split with semicolon)</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-accounts-null-password</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Disable null password user present</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-sshd-root-login</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow remote login of root user via SSH</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-sshd-pubkey-auth</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow SSH password free login</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-sshd-weak-encryption</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow to use SSH weak encryption algorithm</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-sshd-banner-info</name>
            <arg>
                <name>Banner</name>
                <value>"none"</value>
                <value_limits>(none)|(\/([\w\.]+\/?)*)</value_limits>
                <input_example>none || /etc/issue.net</input_example>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>"none"</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Hiding operating system version information</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-pam-su-wheel</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Users outside the wheel group are prohibited from using the 'su root' command</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-hosts-login-limit</name>
            <arg>
                <name>allow-hosts</name>
                <value>""</value>
                <value_limits>((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})){2}\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})?(,((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})){2}\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})?)*</value_limits>
                <input_example>(xxx.yyy.|xxx.yyy.zzz|ip),...</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>List of IP addresses that allow remote access (Split with comma)</_label>
                </layout>
            </arg>
            <arg>
                <name>deny-hosts</name>
                <value>""</value>
                <value_limits>((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})){2}\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})?(,((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})){2}\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})?)*</value_limits>
                <input_example>(xxx.yyy.|xxx.yyy.zzz|ip),...</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
