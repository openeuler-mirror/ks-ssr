        </reinforcement>
        <reinforcement>
            <name>config-umask-limit</name>
            <arg>
                <name>enabled</name>
                <value>027</value>
                <value_limits>022</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <max_value>027</max_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DATETIME</widget_type>
                    <_label>File directory default permission</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-nouser-files</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>No master file in the scanning system</_label>
                </layout>
                <_note>Check if there are files in the /bin,/usr/bin,/sbin,/usr/sbin directories that do not belong to the user</_note>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-authority-files</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Scanning system files with 777 permissions</_label>
                </layout>
                <_note>Scan files with permissions of 777 in the /bin,/usr/bin,/sbin,/usr/sbin directory</_note>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-suid-sgid-files</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Scan system important directory suid-sgid file</_label>
                </layout>
                <_note>Scan files with SUID SGID permissions in the/bin,/usr/bin,/sbin, and/usr/sbin directories of the system</_note>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>config-vulnerability-scanning</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Built in common vulnerability version version number scanning</_label>
                </layout>
            </arg>
            <!-- <arg>
                <name>invalid_rpms</name>
                <value>"openssh;openssl;"</value>
                <value_limits>[^\\\/\^]*</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>"openssh;openssl;"</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>contents</_label>
                </layout>
            </arg> -->
        </reinforcement>
        <!-- 审计类 -->
        <reinforcement>
            <name>audit-auditd-switch</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Turn on system auditd service</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>audit-auditd-rules</name>
            <arg>
                <name>add-path</name>
                <value>""</value>
                            <!-- ^((/[^\\\\/:*?\"<>|]+)*/[^\\\\/:*?\"<>|]+\\.[^\\\\/:*?\"<>|]+,)*(/[^\\\\/:*?\"<>|]+)*/[^\\\\/:*?\"<>|]+\\.[^\\\\/:*?\"<>|]+$ -->
                <value_limits>^\/(\w+\/?)+$</value_limits>
                <input_example>/etc/passwd</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Add audit rules to path.</_label>
                </layout>
            </arg>
            <arg>
                <name>del-path</name>
                <value>""</value>
                <value_limits>^\/(\w+\/?)+$</value_limits>
                <input_example>/etc/passwd</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Delete audit rules to path.</_label>
                </layout>
            </arg>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Delete all audit rules</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>audit-logrotate-rotate</name>
            <arg>
                <name>rotate</name>
                <value>24</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <min_value>24</min_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>System log save cycle（weekly）</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>audit-logfile-permissions</name>
            <arg>
                <name>mode-permissions-limit</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>The maximum permission of log files and configurations is set to 0644</_label>
                </layout>
            </arg>
            <arg>
                <name>append-permissions-limit</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Log files can only be appended</_label>
                </layout>
            </arg>
        </reinforcement>
        <!-- 接入类 -->
        <reinforcement>
            <name>external-udev-cdrom</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow to use CD-ROM device</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-udev-usb</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow to use USB device</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-udev-ttys</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow to use serial port device</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-accounts-login-limit</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Restrict system independent user login</_label>
                </layout>
            </arg>
            <arg>
                <name>permission-users</name>
                <value>""</value>
                <value_limits>[a-zA-Z0-9_.][a-zA-Z0-9_.-]*[$]?([;][a-zA-Z0-9_.][a-zA-Z0-9_.-]*[$]?)*</value_limits>
                <input_example>name1;name2...</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow login to account (separated by semicolons)</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-accounts-null-password</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Disable null password user present</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-accounts-surplus-user</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Delete lp, games, operator and adm by default</_label>
                </layout>
            </arg>
            <arg>
                <name>delete-users</name>
                <value>""</value>
                <value_limits>[a-zA-Z0-9_.][a-zA-Z0-9_.-]*[$]?([;][a-zA-Z0-9_.][a-zA-Z0-9_.-]*[$]?)*</value_limits>
                <input_example>name1;name2...</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Add users to delete (Split with semicolon)</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-sshd-root-login</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Rrohibit remote login of root user via SSH</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-sshd-pubkey-auth</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow SSH password free login</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-sshd-weak-encryption</name>
            <arg>
                <name>enabled</name>
                <value>false</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>false</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Allow to use SSH weak encryption algorithm</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-sshd-banner-info</name>
            <arg>
                <name>Banner</name>
                <value>"none"</value>
                <value_limits>(none)|(\/([\w\.]+\/?)*)</value_limits>
                <input_example>none || /etc/issue.net</input_example>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>"none"</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Hiding operating system version information</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-sshd-service</name>
            <arg>
                <name>protocol</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Enforce V2 security protocol</_label>
                </layout>
            </arg>
            <arg>
                <name>permitEmpty</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Prohibit SSH users with empty password from logging in</_label>
                </layout>
            </arg>
            <arg>
                <name>port</name>
                <value>""</value>
                <value_limits>(((60[0-9]{3})|([1-5][0-9]{4})|([1-9][0-9]{0,3})))</value_limits>
                <input_example>1~60999</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Modify the default SSH port</_label>
                </layout>
            </arg>
            <arg>
                <name>pam</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Enable PAM login mechanism</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-pam-su-wheel</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Users outside the wheel group are prohibited from using the 'su root' command</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-pam-sudo-limits</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Restrict user sudo command permission</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-hosts-login-limit</name>
            <arg>
                <name>allow-hosts</name>
                <value>""</value>
                <value_limits>(((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})){1,2}\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})?){1}(,((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})){2}\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})?)*</value_limits>
                <input_example>192.168.|192.168.1.|192.168.1.1</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>List of IP addresses that allow remote access (Split with comma)</_label>
                </layout>
            </arg>
            <arg>
                <name>deny-hosts</name>
                <value>""</value>
                <value_limits>(((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})){1,2}\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})?){1}(,((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})(\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})){2}\.((2(5[0-5]|[0-4]\d))|[0-1]?\d{1,2})?)*</value_limits>
                <input_example>192.168.|192.168.1.|192.168.1.1</input_example>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>List of IP addresses for which remote access is prohibited (Split with comma)</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-profile-tmout</name>
            <arg>
                <name>ClientAliveInterval</name>
                <value>300</value>
                <value_limits>0</value_limits>
                <rule>
                    <type>RANGE</type>
                    <value_range>
                        <max_value>300</max_value>
                    </value_range>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>If no operation is performed for more than the following time (seconds), the session exits</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-sftp-user</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Whether to configure sftpuser special account</_label>
                </layout>
            </arg>
        </reinforcement>
        <reinforcement>
            <name>external-radio-switch</name>
            <arg>
                <name>enabled</name>
                <value>true</value>
                <value_limits>""</value_limits>
                <rule>
                    <type>FIXED</type>
                    <value_fixed>true</value_fixed>
                </rule>
                <layout>
                    <widget_type>DEFAULT</widget_type>
                    <_label>Disable radio network</_label>
                </layout>
            </arg>
        </reinforcement>
    </body>
