{
    "head": {
        "name": "CUSTOM_CLASS_I_SAFETY_STANDARD",
        "label": "一级安全标准",
        "version": "1.0.0"
    },
    "body": {
        "item_count": 3,
        "items": [
            {
                "name": "config-login-lock",
                "rules": {
                    "deny": [
                        1,
                        6
                    ],
                    "unlock_time": [
                        30,
                        1800
                    ]
                },
                "default_args": {
                    "deny": 3,
                    "unlock_time": 300
                },
                "layout": [
                    {
                        "widget": "label",
                        "posx": 0,
                        "posy": 0,
                        "value": "deny: "
                    },
                    {
                        "widget": "entry",
                        "posx": 1,
                        "posy": 0,
                        "value": "${deny}"
                    },
                    {
                        "widget": "label",
                        "posx": 0,
                        "posy": 1,
                        "value": "unlock_time:"
                    },
                    {
                        "widget": "entry",
                        "posx": 0,
                        "posy": 1,
                        "value": "${unlock_time}"
                    }
                ]
            },
            {
                "name": "config-password-complexity",
                "rules": {
                    "dcredit": [
                        -15,
                        -5
                    ],
                    "ocredit": [
                        -2,
                        -15
                    ]
                },
                "default_args": {
                    "dcredit": -9,
                    "ocredit": -3
                },
                "layout": {}
            },
            {
                "name": "config-password-expired",
                "rules": {
                    "pass_max_days": [
                        60,
                        -1
                    ]
                },
                "default_args": {
                    "pass_max_days": 65
                }
            },
            {
                "name": "network-firewalld-switch",
                "rules": {
                    "enabled": true,
                    "active": true
                },
                "default_args": {
                    "enabled": true,
                    "active": true
                }
            },
            {
                "name": "network-firewalld-icmp-timestamp",
                "rules": {
                    "disable_timestamp_request": true
                },
                "default_args": {
                    "disable_timestamp_request": true
                }
            },
            {
                "name": "network-sysctl-redirect",
                "rules": {
                    "enabled": false
                },
                "default_args": {
                    "enabled": false
                }
            },
            {
                "name": "network-sysctl-source-route",
                "rules": {
                    "enabled": false
                },
                "default_args": {
                    "enabled": false
                }
            }
        ]
    }
}