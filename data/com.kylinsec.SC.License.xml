<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd">
    <interface name="com.kylinsec.SC.License">
        <method name="ActivateByActivationCode">
            <arg type="s" name="active_code" direction="in">
                <summary>Activation code.</summary>
            </arg>
            <description>Activate by activation code.</description>
        </method>

        <method name="GetMachineCode">
            <arg type="s" name="machine_code" direction="out">
                <summary>Machine code.</summary>
            </arg>
            <description>Get machine code.</description>
        </method>

        <method name="GetActivationCode">
            <arg type="s" name="activation_code" direction="out">
                <summary>Activation code.</summary>
            </arg>
            <description>Get activation code.</description>
        </method>

        <method name="GetExpiredTime">
            <arg type="s" name="expired_time" direction="out">
                <summary>Expired time.</summary>
            </arg>
            <description>Get expired time.</description>
        </method>

        <property name="activated" type="b" access="read">
            <description>Is it activated.</description>
        </property>

        <signal name="LicenseChanged">
            <description>License changed.</description>
        </signal>
    </interface>
</node>
