<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<ssr_plugin>
    <name>external</name>
    <category>external</category>
    <_label>external settings</_label>
    <language_type>python</language_type>
    <available>true</available>
    <reinforcement>
        <name>external-udev-cdrom</name>
        <_label>CD-ROM device restrictions</_label>
        <_description>Restrict read and write operations of CD-ROM devices</_description>
    </reinforcement>
    <reinforcement>
        <name>external-udev-usb</name>
        <_label>USB storage device restrictions</_label>
        <_description>Restrict read and write operations of USB storage devices</_description>
    </reinforcement>
    <reinforcement>
        <name>external-udev-ttys</name>
        <_label>Serial port device restrictions</_label>
        <_description>Restrict read and write operations of Serial port devices</_description>
    </reinforcement>
    <reinforcement>
        <name>external-accounts-login-limit</name>
        <_label>User login limit</_label>
        <_description>Restrict system independent user login</_description>
    </reinforcement>
    <reinforcement>
        <name>external-accounts-null-password</name>
        <_label>Null password detection</_label>
        <_description>Avoid null password user in the system</_description>
    </reinforcement>
    <reinforcement>
        <name>external-sshd-root-login</name>
        <_label>Remote login restrictions for root user</_label>
        <_description>Restrict remote login of root user through SSH connection</_description>
    </reinforcement>
    <reinforcement>
        <name>external-sshd-pubkey-auth</name>
        <_label>SSH password free login</_label>
        <_description>Password free login by storing public key</_description>
    </reinforcement>
    <reinforcement>
        <name>external-sshd-weak-encryption</name>
        <_label>SSH weak encryption algorithm</_label>
        <_description>Enhance the security of SSH data transmission and prevent data from being cracked</_description>
    </reinforcement>
    <reinforcement>
        <name>external-pam-su-wheel</name>
        <_label>su command limit</_label>
        <_description>Users outside the wheel group are prohibited from using the 'su root' command</_description>
    </reinforcement>
    <reinforcement>
        <name>external-radio-switch</name>
        <_label>Disable radio network</_label>
    </reinforcement>
</ssr_plugin>