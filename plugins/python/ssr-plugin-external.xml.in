<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<ssr_plugin>
    <name>external</name>
    <category>external</category>
    <_label>external settings</_label>
    <language_type>python</language_type>
    <available>true</available>
    <reinforcement>
        <name>external-udev-cdrom</name>
        <_label>CD-ROM device restrictions</_label>
        <_description>Restrict read and write operations of CD-ROM devices</_description>
    </reinforcement>
    <reinforcement>
        <name>external-udev-usb</name>
        <_label>USB storage device restrictions</_label>
        <_description>Restrict read and write operations of USB storage devices</_description>
    </reinforcement>
    <reinforcement>
        <name>external-udev-ttys</name>
        <_label>Serial port device restrictions</_label>
        <_description>Restrict read and write operations of Serial port devices</_description>
    </reinforcement>
    <reinforcement>
        <name>external-accounts-login-limit</name>
        <_label>User login limit</_label>
        <_description>Restrict system independent user login</_description>
    </reinforcement>
    <reinforcement>
        <name>external-accounts-null-password</name>
        <_label>Null password detection</_label>
        <_description>Avoid null password user in the system</_description>
    </reinforcement>
    <reinforcement>
        <name>external-accounts-surplus-user</name>
        <_label>Delete redundant users</_label>
        <_description>Delete lp, games, operator, adm by default and add and delete by user</_description>
    </reinforcement>
    <reinforcement>
        <name>external-sshd-root-login</name>
        <_label>Remote login restrictions for root user</_label>
        <_description>Restrict remote login of root user through SSH connection</_description>
    </reinforcement>
    <reinforcement>
        <name>external-sshd-pubkey-auth</name>
        <_label>SSH password free login</_label>
        <_description>Password free login by storing public key</_description>
    </reinforcement>
    <reinforcement>
        <name>external-sshd-weak-encryption</name>
        <_label>SSH weak encryption algorithm</_label>
        <_description>Enhance the security of SSH data transmission and prevent data from being cracked</_description>
    </reinforcement>
    <reinforcement>
        <name>external-sshd-banner-info</name>
        <_label>SSH login banner information</_label>
        <_description>Avoid the leakage of operating system version information through SSH</_description>
    </reinforcement>
    <reinforcement>
        <name>external-sshd-service</name>
        <_label>Sshd service reinforcement</_label>
        <_description>Restricted security protocol, empty password user login, SSH port, PAM</_description>
    </reinforcement>
    <reinforcement>
        <name>external-pam-su-wheel</name>
        <_label>su command limit</_label>
        <_description>Users outside the wheel group are prohibited from using the 'su root' command</_description>
    </reinforcement>
    <reinforcement>
        <name>external-pam-sudo-limits</name>
        <_label>Sudo command permissions</_label>
        <_description>Restrict sudo command permissions</_description>
    </reinforcement>
    <reinforcement>
        <name>external-hosts-login-limit</name>
        <_label>Remote IP access restrictions</_label>
        <_description>Set the remote IP that can and cannot access this machine</_description>
    </reinforcement>
    <reinforcement>
        <name>external-profile-tmout</name>
        <_label>Session login timeout limit</_label>
        <_description>If the session does not operate within the specified time after login, it will exit automatically</_description>
    </reinforcement>
    <reinforcement>
        <name>external-sftp-user</name>
        <_label>Limit SFTP</_label>
        <_description>Restrict SFTP users' upward cross directory access</_description>
    </reinforcement>
    <reinforcement>
        <name>external-radio-switch</name>
        <_label>Disable radio network</_label>
        <_description>Restrict wireless network connections</_description>
    </reinforcement>
</ssr_plugin>