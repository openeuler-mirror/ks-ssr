{
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    padding-left: 36px;
}

KS--BR--Scan QTreeView
{
    show-decoration-selected: 1;
    background-color: transparent;
    selection-background-color: #43A3F2;
    font-size: 12px;
    color: white;
    font:NotoSansCJKsc-Regular;
    border: none;
    border-radius: 4px;
}

KS--BR--Scan QTreeView > QWidget
{
    margin: 0px 10px 0px 0px;
}

/* 有子项的item */
KS--BR--Scan QTreeView::item:has-children
{
    margin: 4px 0px 0px 0px;
    background-color: #393939;
    border: 0px;
    padding-left:14px;
}

/* 有子项的第一列 */
KS--BR--Scan QTreeView::item:first:has-children
{
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
}

/* 有子项的第一列展开 */
KS--BR--Scan QTreeView::item:open:first:has-children
{
    border-top-left-radius: 4px;
    border-bottom-left-radius: 0px;
}

/* 有子项的最后一列 */
KS--BR--Scan QTreeView::item:last:has-children
{
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    padding-left:70px;
}
/* 有子项的最后一列展开 */
KS--BR--Scan QTreeView::item:open:last:has-children
{
    border-top-right-radius: 4px;
    border-bottom-right-radius: 0px;
}

/* 没有子项的item */
KS--BR--Scan QTreeView::item:!has-children
{
    margin: 0px 0px 0px 0px;
    background-color: #393939;
    border: 0px;
    padding-left:14px;
}

KS--BR--Scan QTreeView::branch
{
    border: 0px;
    background-color: #393939;
    padding-left:14px;
    margin: 0px 0px 0px 0px;
}

/* 展开后branch最后一项左侧的空白缩进item */
KS--BR--Scan QTreeView::branch:!has-siblings:adjoins-item
{
    border-bottom-left-radius: 4px;
}
/* 展开后items最后一列item */
KS--BR--Scan QTreeView::item:!has-siblings:last
{
    border-bottom-right-radius: 4px;
}

/*
KS--BR--Scan QTreeView::branch:!has-children:!has-siblings:adjoins-item,
KS--BR--Scan QTreeView::branch:has-siblings:adjoins-item
{
    background: red;
    margin: 0px 0px 0px 0px;
}
*/

KS--BR--Scan QTreeView::item:has-children:hover,
KS--BR--Scan QTreeView::item:!has-children:hover,
KS--BR--Scan QTreeView::item:has-children:selected,
KS--BR--Scan QTreeView::item:!has-children:selected,
KS--BR--Scan QTreeView::branch:has-children:hover,
KS--BR--Scan QTreeView::branch:!has-children:hover,
KS--BR--Scan QTreeView::branch:has-children:selected,
KS--BR--Scan QTreeView::branch:!has-children:selected
{
    background-color: #43A3F2;
}

