/* common */

KS--Window
{
    font-size: 12px;
    border-radius: 4px;
    background-color:#2d2d2d;
    color: white;
    font:NotoSansCJKsc-Regular;
}

QPushButton
{
    color: #FFFFFF;
    background-color: #393939;
    border: none;
    border-radius: 4px;
    font-size: 12px;
    font:NotoSansCJKsc-Regular;
}

QPushButton:checked
{
    background-color: #00a2ff;
}

QLabel
{
    color: #FFFFFF;
    font-size: 12px;
    font:NotoSansCJKsc-Regular;
}

QLineEdit
{
    background-color: #222222;
    border: 1px solid #454545;
    border-radius: 6px;
    font-size: 12px;
    color: white;
    font:NotoSansCJKsc-Regular;
}

QToolTip
{
    background-color: #FFFFFF;
    border-radius: 4px;
    font-size: 12px;
    color: #000000;
    font:NotoSansCJKsc-Regular;
}


/* common scrollarea/scrollbar */

QScrollArea
{
    background-color: transparent;
}

QScrollArea > QWidget > QWidget {
    background-color: transparent;
}

QScrollBar:vertical {
    border: none;
    background: transparent;
    margin: 0px;
    width: 4px;
}

QScrollBar::handle:vertical {
    border: none;
    border-radius: 2px;
    background-color: #595959;
    min-width: 4px;
    min-height:30px;
    margin: 0px;
}

QScrollBar::handle:vertical:hover {
    background-color: #696969;
}

QScrollBar::handle:vertical:pressed  {
    background-color: #2eb3ff;
}

QScrollBar::add-line:vertical {
    border: none;
    background: none;
}

QScrollBar::sub-line:vertical {
    border: none;
    background: none;
}

/* common table */
QHeaderView,
QTableView
{
    background-color: transparent;
    font-size: 12px;
    color: white;
    font:NotoSansCJKsc-Regular;
}

QHeaderView#tableHeaderProxy
{
    color: #919191;
}

/* SideBar */
KS--SideBar
{
    font-size: 12px;
    color: white;
    font:NotoSansCJKsc-Regular;
    background-color:#2d2d2d;
    border-radius: 6px;
    padding-left: 10px;
    padding-top: 16px;
    padding-right: 24px;
}
KS--SideBar::item
{
    border: 0px;
    background-color: #393939;
    margin: 2px 0px 2px 0px;
    border-radius: 6px;
    border: 6px
}

KS--SideBar::item::selected
{
    background: #00a2ff;
}

QHeaderView::section
{
    background-color: #393939;
    border: none;
    margin-bottom: 2px;
    padding-left:10px;
}

KS--DM--DeviceLogPage QHeaderView::section:first,
KS--DM--DeviceListPage QHeaderView::section:first
{
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
}

KS--TP--KernelProtectedPage QHeaderView::section:first,
KS--TP--ExecuteProtectedPage QHeaderView::section:first,
KS--FP--FileProtectionPage QHeaderView::section:first
{
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    padding-left:-2px;
}

QHeaderView::section:last
{
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}

QTableView::item
{
    border: 0px;
    /*background-color: #393939;*/
    background-color: transparent;
    margin: 2px 0px 2px 0px;
    padding-left:10px;
}

/* common QCheckBox */

QCheckBox::indicator:unchecked {
    image: url(:/images/checkbox-unchecked-normal);
}

QCheckBox::indicator:unchecked:hover {
    image: url(:/images/checkbox-unchecked-hover);
}

QCheckBox::indicator:unchecked:pressed {
    image: url(:/images/checkbox-unchecked-active);
}

QCheckBox::indicator:checked {
    image: url(:/images/checkbox-checked-normal);
}

QCheckBox::indicator:checked:hover {
    image: url(:/images/checkbox-checked-hover);
}

QCheckBox::indicator:checked:pressed {
    image: url(:/images/checkbox-checked-active);
}

QCheckBox::indicator:indeterminate:hover {
    image: url(:/images/checkbox-indeterminate-hover);
}

QCheckBox::indicator:indeterminate:pressed {
    image: url(:/images/checkbox-indeterminate-active);
}
QCheckBox::indicator::disabled:checked{
    image: url(:/images/checkbox-checked-disabled);
}
QCheckBox::indicator::disabled:unchecked{
    image: url(:/images/checkbox-unchecked-disabled);
}

/* common menu */
QMenu
{
    font:NotoSansCJKsc-Regular;
